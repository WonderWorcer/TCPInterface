            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </Label>
        <TableView fx:id="tableView" layoutY="109.0" maxHeight="1.7976931348623157E308" prefHeight="491.0" prefWidth="800.0">
            <columns>
                <TableColumn prefWidth="270.0" text="Адрес регистра">
                    <cellValueFactory>
                        <PropertyValueFactory property="address" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="515.0" text="Значение">
                    <cellValueFactory>
                        <PropertyValueFactory property="value" />
                    </cellValueFactory>
                </TableColumn>
