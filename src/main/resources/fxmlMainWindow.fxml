<GridPane alignment="center" hgap="10" minHeight="500.0" minWidth="600.0" prefHeight="500.0" prefWidth="600.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">

    <children>
        <Label prefHeight="29.0" prefWidth="248.0" style="-fx-font: NORMAL 20 Tahoma;" text="TCP/IP 08-24TC" GridPane.columnIndex="0" GridPane.rowIndex="0">
        </Label>
        <TableView fx:id="tableView" prefHeight="400.0" prefWidth="333.0" GridPane.rowIndex="2">

            <columns>
                <TableColumn prefWidth="116.0" text="FlagName">
                    <cellValueFactory><PropertyValueFactory property="flagName" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="98.0" text="Adress">
                    <cellValueFactory><PropertyValueFactory property="adress" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="118.0" text="Value">
                    <cellValueFactory><PropertyValueFactory property="value" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
            <items>
                <FXCollections fx:factory="observableArrayList">

                </FXCollections>

            </items>
        </TableView>
      <Button mnemonicParsing="false" onAction="#addAllData" prefHeight="15.0" prefWidth="63.0" text="Get Data" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      <TextField fx:id="ipAdress" onAction="#addAllData" prefHeight="25.0" prefWidth="17.0" GridPane.rowIndex="1" />
      <TextField fx:id="port" onAction="#addAllData" prefHeight="25.0" prefWidth="143.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />


    </children>
    <columnConstraints>
        <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="67.0" minHeight="1.0" prefHeight="29.0" />
        <RowConstraints maxHeight="67.0" minHeight="1.0" prefHeight="29.0" />
      <RowConstraints maxHeight="428.0" minHeight="362.0" prefHeight="400.0" />
    </rowConstraints>

</GridPane>
