                <Font size="14.0" />
            </font>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </TextField>
        <TextField fx:id="port" layoutX="198.0" layoutY="66.0" minHeight="30.0" promptText="Порт" text="502">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
      <Button layoutX="698.0" layoutY="66.0" mnemonicParsing="false" onAction="#showSetWindow" text="Настройка">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
